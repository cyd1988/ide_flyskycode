{
    "name": "vscode-plugin-flyskycode",
    "displayName": "vscode-plugin-flyskycode",
    "description": "cyd1988",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.41.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "Code插件demo",
            "properties": {
                "vscodePluginDemo.yourName": {
                    "type": "string",
                    "default": "guest",
                    "description": "你的名字"
                },
                "vscodePluginDemo.showTip": {
                    "type": "boolean",
                    "default": true,
                    "description": "启动时显示自定义欢迎页"
                }
            }
        },
        "commands": [
            {
                "command": "extension.sayHello",
                "title": "Hello，小茗同学"
            },
            {
                "command": "extension.demo.getCurrentFilePath",
                "title": "获取当前文件(夹)路径"
            },
            {
                "command": "extension.demo.testMenuShow",
                "title": "这个菜单仅在JS文件中出现",
                "icon": {
                    "light": "./images/tool-light.svg",
                    "dark": "./images/tool-light.svg"
                }
            },
            {
                "command": "extension.demo.openWebview",
                "title": "打开WebView"
            },
            {
                "command": "extension.demo.showWelcome",
                "title": "显示自定义欢迎页"
            },
            {
                "command": "extension.demo.openAuto",
                "title": "打开文件"
            },
            {
                "command": "extension.demo.openProject",
                "title": "new project"
            }
        ],
        "keybindings": [
            {
                "command": "extension.demo.openWebview",
                "key": "ctrl+f9",
                "mac": "cmd+f9",
                "when": "editorTextFocus"
            },
            {
                "command": "extension.demo.openAuto",
                "key": "alt+f5",
                "mac": "alt+f5",
                "when": "editorTextFocus"
            },
            {
                "command": "extension.demo.openAutoCopy",
                "key": "alt+f alt+c",
                "mac": "alt+f alt+c",
                "when": "editorTextFocus"
            },
            {
                "command": "extension.demo.runBash",
                "key": "alt+shift+t",
                "mac": "alt+shift+t",
                "when": "editorTextFocus"
            },
            {
                "command": "extension.demo.runBashSsh",
                "key": "ctrl+r ctrl+h",
                "mac": "ctrl+r ctrl+h",
                "when": "editorTextFocus"
            },
            {
                "command": "extension.demo.fileOpen",
                "key": "alt+f1",
                "mac": "alt+f1",
                "args": "af1",
                "when": "editorTextFocus"
            },
            {
                "command": "extension.demo.fileOpen",
                "key": "alt+f2",
                "mac": "alt+f2",
                "args": "af2",
                "when": "editorTextFocus"
            },
            {
                "command": "extension.demo.fileOpen",
                "key": "alt+f3",
                "mac": "alt+f3",
                "args": "af3",
                "when": "editorTextFocus"
            },
            {
                "command": "extension.demo.fileOpen",
                "key": "alt+f4",
                "mac": "alt+f4",
                "args": "af4",
                "when": "editorTextFocus"
            },
            {
                "command": "extension.demo.fileOpen",
                "key": "cmd+0",
                "mac": "cmd+0",
                "args": "cmd0",
                "when": "editorTextFocus"
            },
            {
                "command": "extension.demo.fileOpen",
                "key": "cmd+1",
                "mac": "cmd+1",
                "args": "cmd1",
                "when": "editorTextFocus"
            },
            {
                "command": "extension.demo.fileOpen",
                "key": "cmd+2",
                "mac": "cmd+2",
                "args": "cmd2",
                "when": "editorTextFocus"
            },
            {
                "command": "extension.demo.fileOpen",
                "key": "cmd+3",
                "mac": "cmd+3",
                "args": "cmd3",
                "when": "editorTextFocus"
            },
            {
                "command": "extension.demo.fileOpen",
                "key": "cmd+4",
                "mac": "cmd+4",
                "args": "cmd4",
                "when": "editorTextFocus"
            },
            {
                "command": "extension.demo.fileOpen",
                "key": "cmd+5",
                "mac": "cmd+5",
                "args": "cmd5",
                "when": "editorTextFocus"
            },
            {
                "command": "extension.demo.fileOpen",
                "key": "cmd+6",
                "mac": "cmd+6",
                "args": "cmd6",
                "when": "editorTextFocus"
            },
            {
                "command": "extension.demo.fileOpen",
                "key": "cmd+7",
                "mac": "cmd+7",
                "args": "cmd7",
                "when": "editorTextFocus"
            },
            {
                "command": "extension.demo.fileOpen",
                "key": "cmd+8",
                "mac": "cmd+8",
                "args": "cmd8",
                "when": "editorTextFocus"
            },
            {
                "command": "extension.demo.fileOpen",
                "key": "cmd+9",
                "mac": "cmd+9",
                "args": "cmd9",
                "when": "editorTextFocus"
            },
            {
                "command": "extension.demo.fileOpen",
                "key": "cmd+-",
                "mac": "cmd+-",
                "args": "cmd-",
                "when": "editorTextFocus"
            },
            {
                "command": "extension.demo.fileOpen",
                "key": "cmd+=",
                "mac": "cmd+=",
                "args": "cmd=",
                "when": "editorTextFocus"
            },
            {
                "command": "extension.demo.debugPrint",
                "key": "cmd+shift+g",
                "mac": "cmd+shift+g",
                "args": {"end":0},
                "when": "editorTextFocus"
            },
            {
                "command": "extension.demo.debugPrint",
                "key": "cmd+shift+h",
                "mac": "cmd+shift+h",
                "args": {"end":1},
                "when": "editorTextFocus"
            },
            {
                "command": "extension.demo.systemEdit",
                "key": "backspace",
                "mac": "backspace",
                "args": {"ty":"backspace"},
                "when": "editorTextFocus"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "when": "editorFocus && resourceLangId == javascript",
                    "command": "extension.demo.testMenuShow",
                    "group": "z_commands"
                },
                {
                    "command": "extension.demo.openWebview",
                    "group": "navigation"
                }
            ],
            "editor/title": [
                {
                    "when": "editorFocus && resourceLangId == javascript",
                    "command": "extension.demo.testMenuShow",
                    "group": "navigation"
                }
            ],
            "editor/title/context": [
                {
                    "when": "resourceLangId == javascript",
                    "command": "extension.demo.testMenuShow",
                    "group": "navigation"
                }
            ],
            "explorer/context": [
                {
                    "command": "extension.demo.openProject",
                    "group": "navigation"
                }
            ]
        },
        "snippets": [
            {
                "language": "javascript",
                "path": "./snippets/javascript.json"
            },
            {
                "language": "html",
                "path": "./snippets/html.json"
            }
        ],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "beautifulGirl",
                    "title": "美女",
                    "icon": "images/beautifulGirl.svg"
                }
            ]
        },
        "iconThemes": [
            {
                "id": "testIconTheme",
                "label": "测试图标主题",
                "path": "./theme/icon-theme.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.41.0",
        "glob": "^7.1.5",
        "mocha": "^6.2.2",
        "typescript": "^3.6.4",
        "tslint": "^5.20.0",
        "vscode-test": "^1.2.2"
    },
    "dependencies": {
        "@types/mysql": "^2.15.8",
        "@types/redis": "^2.8.14",
        "mysql": "^2.17.1",
        "redis": "^2.8.0"
    }
}
